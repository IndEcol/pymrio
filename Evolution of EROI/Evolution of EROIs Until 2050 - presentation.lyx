\begin_deeper
\begin_layout Standard
The element 
\begin_inset Formula $a_{i,j}$
\end_inset

 of the technology matrix 
\emph on
A
\emph default
 represents the quantity of input 
\emph on
i
\emph default
 required to produce one unit of output 
\emph on
j
\emph default
.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\overset{A=\left(a_{\text{in, out}}\right)=\begin{pmatrix}a_{1,1} & \cdots & a_{1,n}\\
\vdots & \ddots & \vdots\\
a_{n,1} & \ldots & a_{n,n}
\end{pmatrix}\leftarrow\text{ inputs}}{\underset{\text{outputs}}{\downarrow}}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
A=\begin{pmatrix}0 & 0 & 1\\
m_{e} & m_{m} & 0\\
E_{e} & E_{m} & 0
\end{pmatrix}=\begin{pmatrix}0 & 0 & 1\\
m_{e} & 0.2 & 0\\
0.1 & 0.5 & 0
\end{pmatrix}\begin{array}{c}
\text{energy techno.}\\
\text{materials}\\
\text{energy}
\end{array}
\]

\end_inset


\end_layout

\end_deeper
