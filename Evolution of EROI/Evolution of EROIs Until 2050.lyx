\end_layout

\end_inset


\begin_inset Note Note
status open

\begin_layout Plain Layout
cf.
 Herendeen 2015
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\text{EROI} & =\frac{\left(1-E_{e}\right)\left(m_{m}-1\right)+E_{m}m_{e}}{E_{e}\left(m_{m}-1\right)-E_{m}m_{e}}\\
 & =\frac{0.72-0.5m_{e}}{0.08+0.5m_{e}}
\end{align*}

